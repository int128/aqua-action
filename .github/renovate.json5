{
  "extends": [
    "config:base",
    "github>suzuki-shunsuke/aqua-renovate-config",
    "github>int128/typescript-action-renovate-config"
  ],
  regexManagers: [
    {
      fileMatch: [
        "README.md",
        "action.yaml",
      ],
      matchStrings: [
        ": (?<currentValue>.*?) # renovate: depName=(?<depName>.*?)\\n",
      ],
      datasourceTemplate: "github-releases",
    },
  ],
}
